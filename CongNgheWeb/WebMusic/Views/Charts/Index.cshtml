    Layout = "~/Views/Layout/ChartStemLayout.cshtml";
}

<link href="~/Content/Css/charts.css" rel="stylesheet" />
<div class="content">
    <div class="container">
        <div class="row">
            <div class="col-xs-12">
                <div class="content-top">
                    <div class="pull-left">
                        <h1>DJ CHARTS</h1>
                    </div>
                    <div class="pull-right">
                        <ul>
                            <li class="active"><a href="#">FEATURED</a></li>
                            <li><a href="#">ALL DJ CHARTS</a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="container">
        <div class="row">
            <div class="col-xs 12">
                <div class="col-xs-8">
                    @Html.Action("SliderChartsPartial", "Charts");
                    @Html.Action("NewChartsPartial", "Charts");
                </div>
                <div class="col-xs-4">
                    @Html.Action("ChartsGenrePArtial", "Charts");
                </div>
            </div>
        </div>
    </div>
</div>
<script src="~/Content/Javascript/chart.js"></script>


